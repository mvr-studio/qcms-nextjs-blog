{
  "name": "qcms-nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn qcms:start\" \"next dev\"",
    "build": "yarn qcms:generate && next build",
    "start": "concurrently \"yarn qcms:start\" \"next start\"",
    "lint": "next lint",
    "qcms:generate": "dotenv -- yarn --cwd node_modules/@mvr-studio/qcms generate",
    "qcms:start": "dotenv -- yarn --cwd node_modules/@mvr-studio/qcms local:start",
    "qcms:studio": "dotenv -- yarn --cwd node_modules/@mvr-studio/qcms local:studio"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mvr-studio/protochakra": "^0.1.2",
    "@mvr-studio/qcms": "^0.0.6",
    "focus-visible": "^5.2.0",
    "framer-motion": "^6",
    "graphql-request": "^4.1.0",
    "graphql-tag": "^2.12.6",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "concurrently": "^7.0.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "prettier": "^2.5.1",
    "typescript": "4.6.2"
  }
}
